<!-- About Section Begin -->
@{ 
    ViewBag.Title = "Home";
}
<section class="home-about spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-7">
                <div class="home__about__pic">
                    <div class="home__about__pic__item large-item set-bg" data-setbg="/Content/img/break2.jpg"></div>
                    <div class="home__about__pic__item">
                        <div class="home__about__pic__item__inner set-bg" data-setbg="/Content/img/break1.jpg"></div>
                        <div class="home__about__pic__item__inner set-bg" data-setbg="/Content/img/break3.jpg"></div>
                    </div>
                </div>
            </div>
            <div class="col-lg-5">
                <div class="home__about__text">
                    <div class="section-title">
                        <img src="~/Content/img/BreakTime.jpg" alt="Logo"/>
                        <h2>Welcome to BreakTime</h2>
                    </div>
                    <span>“What hurts today makes you stronger tomorrow.”</span>
                    <p>
                        BreakTime offers the best teachers that breaking has to offer. We have professional Bboys from all over the world teaching you not only the basics of breaking but also sharing tips of their success, mentality, and creativity.
                    </p>
                    <p class="para-2">
                        BreakTime is the perfect place for people who love all forms of dancing, people who have just started dancing and people who have been dancing for years.
                    </p>
                    <a href="@Url.Action("About","Home")" class="primary-btn">MORE ABOUT US</a>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- About Section End -->
<!-- Services Section Begin -->
<div class="row"><div class="col-md-12 col-lg-12 video"><iframe width="750" height="420" src="https://www.youtube.com/embed/4kTjyunLIaE?start=118" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe></div></div>
<section class="services spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
